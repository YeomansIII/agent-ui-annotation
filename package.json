{
  "name": "agent-ui-annotation",
  "version": "0.1.0",
  "description": "Web page annotation toolbar for AI coding agents",
  "type": "module",
  "main": "./dist/agent-ui-annotation.umd.cjs",
  "module": "./dist/agent-ui-annotation.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/agent-ui-annotation.js",
      "require": "./dist/agent-ui-annotation.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/adapters/react/AgentUIAnnotation.js",
      "types": "./dist/adapters/react/AgentUIAnnotation.d.ts"
    },
    "./vanilla": {
      "import": "./dist/adapters/vanilla/index.js",
      "types": "./dist/adapters/vanilla/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "jsdom": "^27.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "keywords": [
    "annotation",
    "toolbar",
    "web-component",
    "ai",
    "coding-agent"
  ],
  "license": "MIT"
}
