{
  "name": "agent-ui-annotation",
  "version": "0.6.1",
  "description": "Web page annotation toolbar for AI coding agents",
  "type": "module",
  "main": "./dist/agent-ui-annotation.js",
  "module": "./dist/agent-ui-annotation.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/agent-ui-annotation.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/adapters/react/AgentUIAnnotation.js",
      "types": "./dist/adapters/react/AgentUIAnnotation.d.ts"
    },
    "./vanilla": {
      "import": "./dist/adapters/vanilla/index.js",
      "types": "./dist/adapters/vanilla/index.d.ts"
    },
    "./vue": {
      "import": "./dist/adapters/vue/index.js",
      "types": "./dist/adapters/vue/index.d.ts"
    },
    "./svelte": {
      "svelte": "./dist/adapters/svelte/index.js",
      "import": "./dist/adapters/svelte/index.js",
      "types": "./dist/adapters/svelte/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "dev:demo": "vite --config vite.config.demo.ts",
    "build": "tsc && vite build",
    "build:demo": "vite build --config vite.config.demo.ts",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "eslint": "^9.39.2",
    "jsdom": "^27.4.0",
    "svelte": "^5.37.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.53.1",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^3.0.0",
    "vue": "^3.5.27"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "svelte": ">=5.0.0",
    "vue": ">=3.3.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  },
  "keywords": [
    "annotation",
    "toolbar",
    "web-component",
    "ai",
    "coding-agent"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YeomansIII/agent-ui-annotation"
  },
  "homepage": "https://github.com/YeomansIII/agent-ui-annotation#readme",
  "bugs": {
    "url": "https://github.com/YeomansIII/agent-ui-annotation/issues"
  },
  "dependencies": {
    "lit": "^3.3.2"
  }
}
